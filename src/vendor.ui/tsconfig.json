{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "noEmitOnError": true,
    "inlineSourceMap": false,
    "outDir": "wwwroot/app/"
  },
  "exclude": [
    "wwwroot",
    "node_modules"
  ]
}